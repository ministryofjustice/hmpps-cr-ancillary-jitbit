{
	"Logging": {
		"LogLevel": {
				"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
		},
	"AllowedHosts": "*",

	"SendEmailErrorReports": "false",
	"EmailErrorTo": "support@myserver.com",
	"EmailErrorFrom": "admin@myserver.com",
	"EmailErrorSMTPHost": "mail.myserver.com",
	"EmailErrorSMTPPort": "25",
	"EmailErrorUseSSL": "false",
	"EmailErrorSMTPUser": "admin@myserver.com",
	"EmailErrorSMTPPassword": "password",
	"EmailErrorPOPHost": "mail.myserver.com",
	"EmailErrorPOPPort": "110",
	"EmailErrorPOPUser": "user",
	"EmailErrorPOPPsw": "password",
	"EmailErrorPOPUseSSL": "false",
	"LogErrors": "true",
	"AttachmentsPath": "D:\\OfficeAdmin\\",
	"DisableMailChecker": "",
	"DisallowChangingAvatars": "false",
	"AppURL": "https://${app_url}/",
	"WindowsAdminUsername": "DOMAIN\\Username",
	"MailPswEncryptKey": "",
	"DisableWeakCiphers": "false",
	"UnobtrusiveJavaScriptEnabled": "true",
	"PreserveLoginUrl": "true",

	"ConnectionStrings": {
		"DBConnectionString": "user id=${db_user_id};data source=${db_source};initial catalog=Office;password=${db_user_password}"
	}
}